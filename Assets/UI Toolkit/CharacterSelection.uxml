    <ui:VisualElement name="VisualElement" class="character-selection-root" style="background-image: url(&apos;project://database/Assets/Media/Images/backgroundMenu.png?fileID=2800000&amp;guid=bc734d07967a7ca72ac2505592137887&amp;type=3#backgroundMenu&apos;); opacity: 1;">
