<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xmlns="UnityEngine.UIElements" editor-extension-mode="False">
    <Style src="project://database/Assets/Ui/CharacterSelection.uss?fileID=7433441132597879392&amp;guid=85e77fc156ab8e22a8fab2e3b9ab9c9a&amp;type=3#CharacterSelection" />
